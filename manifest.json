  {
    "name": "NoReTweets",
    "version": "0.00.0002",
    //"default_locale": "en",
    "description": "Read only own person's thoughts",

    "manifest_version": 2,

    "permissions": ["tabs", "activeTab"],
    "icons": {
        "16": "icons/nort_16.png",
        "32": "icons/nort_32.png"
    },

    "browser_action": {
      //"default_icon": "icons/icon.png",
      "default_popup": "popup.html"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    }
  }